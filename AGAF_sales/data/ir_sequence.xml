<odoo>
    <data>
        <record id="seq_estimation_works" model="ir.sequence">
            <field name="name">Estimation Works</field>
            <field name="code">estimation.works</field>
            <field name="prefix">EST/%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="boq_auto_cron" model="ir.cron">
            <field name="name">Boq Compute </field>
            <field name="model_id" ref="model_product_product"/>
            <field name="state">code</field>
            <field name="code">model.bom_compute_func()</field>
            <!-- <field name="user_id" ref="base.user_root"/> -->
            <field name='interval_number'>1</field>
            <field name='interval_type'>days</field>
            <field name="numbercall">-1</field>
            <!-- <field name="doall" eval="False"/> -->
        </record>

        <record id="seq_project_id" model="ir.sequence">
            <field name="name">Project Sequence</field>
            <field name="code">project.project.seq</field>
            <field name="prefix">PRO-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>