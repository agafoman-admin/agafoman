<odoo>
    <data>
        <record id="paperformat_consolidated_picking" model="report.paperformat">
            <field name="name">Consolidated Picking</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">34</field>
            <field name="margin_bottom">18</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_spacing">30</field>
            <field name="dpi">85</field>
        </record>
        <report id="consolidated_picking_report"
                string="Consolidated Picking"
                model="picking.management"
                report_type="qweb-pdf"
                file="AGAF_Project.consolidated_picking_template"
                name="AGAF_Project.consolidated_picking_template"
                print_report_name="'Consolidated Picking- %s' % (object.name)"
                menu="False"
        />

        <record id="AGAF_Project.consolidated_picking_report" model="ir.actions.report">
            <field name="paperformat_id" ref="AGAF_Project.paperformat_consolidated_picking"/>
        </record>
    </data>
</odoo>